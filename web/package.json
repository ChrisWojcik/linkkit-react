{
  "name": "linkkit-react-web",
  "version": "0.0.0",
  "description": "Frontend for a reddit clone",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f ../local-dev-env/.env nodemon -r tsconfig-paths/register ./src/server.ts --watch './src/server.ts'",
    "build": "webpack --mode=production",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:fix": "npm run fix:ts && npm run fix:styles",
    "lint:ts": "eslint --ext .ts,.js,.jsx,.tsx .",
    "fix:ts": "npm run lint:ts -- --fix",
    "lint:styles": "stylelint '**/*.{css,scss}'",
    "fix:styles": "npm run lint:styles -- --fix",
    "prettier": "prettier --check '**/*.{css,scss,js,ts,jsx,tsx}'",
    "prettier:write": "prettier --write '**/*.{css,scss,js,ts,jsx,tsx}'"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "cookie-parser": "^1.4.6",
    "cross-fetch": "^3.1.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.39.4",
    "react-query": "^3.39.2",
    "react-router": "^6.9.0",
    "react-router-dom": "^6.9.0",
    "tabbable": "^6.0.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.9",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-helmet": "^6.1.5",
    "@types/validator": "^13.7.10",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.18.0",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "css-loader": "^6.7.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "mini-css-extract-plugin": "^2.7.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "stylelint": "^14.15.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard-scss": "^6.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-hot-middleware": "^2.25.3"
  }
}
